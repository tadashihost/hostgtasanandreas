public OnPlayerCommandText(playerid, cmdtext[])
{
	if(strcmp(cmdtext, "/halfpipe", true) == 0) {
	SetPlayerPos(playerid, 2039.905,-2581.177,13.311);
	return SetPlayerInterior(playerid,0);
	}

	return 0;
}
	
	
	
	
	
	
	
	
